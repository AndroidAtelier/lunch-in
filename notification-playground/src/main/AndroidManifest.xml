<manifest
    xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.github.androidatelier.lunchin.notification_playground">
  <application
      android:label="@string/app_name"
      android:theme="@style/AppTheme"
      android:allowBackup="true">
    <activity
        android:name="com.github.androidatelier.lunchin.notification_playground.MainActivity"
        android:label="@string/app_name">
      <intent-filter>
        <action android:name="android.intent.action.MAIN"/>
        <category android:name="android.intent.category.LAUNCHER"/>
      </intent-filter>
    </activity>
    <activity android:name="com.github.androidatelier.lunchin.notification_playground.NotificationActivity"/>
    <receiver
        android:name="com.github.androidatelier.lunchin.notification.NotificationReceiver"
        android:exported="false"/>
  </application>
</manifest>